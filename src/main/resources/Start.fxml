<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<VBox fx:id="startRoot" alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="StartController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>


    <TextField promptText="Enter License Number" fx:id="license"/>
    <TextField promptText="Enter Color" fx:id="color"/>
    <TextField promptText="Enter Make" fx:id="make"/>
    <TextField promptText="Enter Model" fx:id="model"/>
    <TextField promptText="Enter Year" fx:id="year"/>

    <HBox alignment="CENTER" spacing="20.0">
        <Button text="Submit" onAction="#Submit"/>
        <Button text="Back" onAction="#Back"/>
    </HBox>

</VBox>
