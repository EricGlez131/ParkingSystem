<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<VBox fx:id="helpRoot" alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="DisplayCarsController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>


    <ScrollPane prefHeight="200.0" prefWidth="300.0">
        <content>
            <TextArea fx:id="stringLabel" editable="false" wrapText="true"/>
        </content>
    </ScrollPane>
    <Button text="Back" onAction="#Back"/>

</VBox>
