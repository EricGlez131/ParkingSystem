<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox fx:id="root" alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button text="Start Session" onAction="#startSession" prefWidth="200"/>
    <Button text="Car Lookup" onAction="#Lookup" prefWidth="200"/>
    <Button text="End / Payment" onAction="#PaymentNend" prefWidth="200"/>
    <Button text="Display Cars" onAction="#displayCar" prefWidth="200"/>


</VBox>