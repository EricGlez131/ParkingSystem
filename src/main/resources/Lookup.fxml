<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<VBox fx:id="findRoot" alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="LookupController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <TextField promptText="Please enter your license for lookup" fx:id="lookUpField"/>
    <Label text="" fx:id="displayLabel"/>

    <HBox alignment="CENTER" spacing="20.0">
        <Button text="Search" onAction="#search"/>
        <Button text="Back" onAction="#Back"/>
    </HBox>

</VBox>
